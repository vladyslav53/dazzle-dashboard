{
  "name": "Dazzle",
  "version": "0.1.0",
  "description": "The simple yet flexible dashbording solution for React",
  "license": "MIT",
  "engines": {
    "node": "4.x"
  },
  "scripts": {
    "postinstall": "npm run build",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production webpack -p",
    "clean": "rimraf dist coverage",
    "start": "cross-env NODE_ENV=production node server/node-server.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server -d --inline --hot --progress --no-info",
    "lint": "npm run lint-js",
    "lint-js": "eslint .",
    "test": "npm run lint && npm run cover",
    "test:watch": "babel-node ./node_modules/mocha/bin/_mocha --opts ./test/mocha.opts --watch",
    "cover": "babel-node ./node_modules/istanbul/lib/cli cover --report html ./node_modules/mocha/bin/_mocha --  ./test/**/*.spec.js",
    "cover2": "cross-env NODE_ENV=test babel-node ./node_modules/istanbul/lib/cli cover node_modules/mocha/bin/_mocha -- --bail --recursive ./test/**/*.spec.js"
  },
  "keywords": [
    "react",
    "dashboard",
    "react-component",
    "widgets"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Raathigesh/Dazzle.git"
  },
  "bugs": {
    "url": "https://github.com/Raathigesh/Dazzle/issues"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "autoprefixer-core": "^6.0.1",
    "babel": "^6.3.26",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.4.0",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.1",
    "babel-plugin-syntax-object-rest-spread": "^6.5.0",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "chai": "^3.4.0",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.0",
    "enzyme": "^2.2.0",
    "es5-shim": "^4.5.6",
    "es6-promise": "^3.1.2",
    "es6-shim": "^0.35.0",
    "eslint": "2.5.2",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-react": "^4.2.0",
    "file-loader": "^0.8.4",
    "history": "^2.0.1",
    "html-webpack-plugin": "^2.9.0",
    "istanbul": "^1.0.0-alpha.2",
    "jsdom": "^8.1.0",
    "json-loader": "^0.5.3",
    "mocha": "^2.3.4",
    "react": "^0.14.7",
    "react-addons-test-utils": "^0.14.3",
    "react-dom": "^0.14.7",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.0.6",
    "react-router": "^2.0.0-rc5",
    "react-router-redux": "^4.0.0",
    "redux": "^3.3.1",
    "redux-form": "^4.1.6",
    "redux-localstorage": "^0.4.0",
    "redux-logger": "^2.1.4",
    "redux-thunk": "^2.0.1",
    "require-hacker": "^2.1.2",
    "rimraf": "^2.4.3",
    "sinon": "^1.17.3",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.0",
    "stylelint": "^5.2.1",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.6.4"
  },
  "dependencies": {
    "classnames": "^2.2.3",
    "express": "^4.13.3",
    "flexboxgrid": "^6.3.0",
    "helmet": "^1.1.0",
    "http-proxy": "^1.12.1",
    "lodash.clonedeep": "^4.3.2",
    "winston": "^2.1.1"
  }
}
